{
  "name": "contributte/scheduler",
  "description": "PHP job scheduler (cron) with locking",
  "keywords": [
    "nette",
    "cron"
  ],
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/contributte/scheduler",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Josef Benjac",
      "homepage": "http://josefbenjac.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "contributte/di": "^0.4.0",
    "dragonmantank/cron-expression": "^2.2.0",
    "symfony/console": "^4.2.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.2.0",
    "ninjify/qa": "^0.9.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.2.1",
    "tracy/tracy": "~2.6.2"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Scheduler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Contributte\\Scheduler\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6.x-dev"
    }
  }
}
